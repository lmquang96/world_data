{
  "name": "admin.world-data",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --inspect ./bin/www",
    "watch": "node-sass --watch src/scss/ --output public/css/",
    "beautiful": "prettier --single-quote --trailing-comma all --tab-width 2 --write **/*.{js,json,scss}"
  },
  "lint-staged": {
    "**/*.{js,json,scss}": "prettier --single-quote --trailing-comma all --tab-width 2 --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0"
  }
}
